{
  "comments": {
    },
  "brackets": [
    ["(", ")"]
  ],
  "autoClosingPairs": [
    {
      "open": "\"",
      "close": "\""
    },
    {
      "open": "'",
      "close": "'"
    },
    {
      "open": "(",
      "close": ")"
    }
  ],
  "surroundingPairs": [
    {
      "open": "\"",
      "close": "\""
    },
    {
      "open": "'",
      "close": "'"
    },
    {
      "open": "(",
      "close": ")"
    }
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(IDENTIFICATION|ENVIRONMENT|DATA|PROCEDURE)\\s+DIVISION\\.",
      "end": "^\\s*END\\s+PROGRAM(?![\\w-])"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(IF|EVALUATE|PERFORM|DO)(?![\\w-]).*(THEN)?\\s*$",
    "decreaseIndentPattern": "^\\s*(END-IF|END-EVALUATE|END-PERFORM)(?![\\w-])"
  },
  "onEnterRules": [
    {
      "beforeText": "^\\s*\\*.*$",
      "action": { "indentAction": "none", "indent": "none" }
    }
  ]
}